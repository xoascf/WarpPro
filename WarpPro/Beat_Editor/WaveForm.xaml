<?xml version="1.0" encoding="utf-8"?>
<UserControl x:Class="Beat_Editor.WaveForm" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:Beat_Editor" Focusable="True" IsTabStop="True">
  <Grid>
    <Canvas Name="scaleCanvas" Background="{StaticResource CanvasBackground}" SnapsToDevicePixels="True">
      <UIElement.CacheMode>
        <BitmapCache />
      </UIElement.CacheMode>
    </Canvas>
    <Canvas Name="waveformCanvas" Background="#00FFFFFF" SnapsToDevicePixels="True" SizeChanged="waveformCanvas_SizeChanged">
      <UIElement.CacheMode>
        <BitmapCache />
      </UIElement.CacheMode>
    </Canvas>
    <Canvas Name="progressCanvas" Background="#00FFFFFF" SnapsToDevicePixels="True" />
    <Canvas Name="selectionCanvas" Background="#00FFFFFF" SnapsToDevicePixels="True" />
  </Grid>
</UserControl>